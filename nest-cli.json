{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-jsapi/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nest-jsapi/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nest-jsapi",
  "projects": {
    "nest-jsapi": {
      "type": "application",
      "root": "apps/nest-jsapi",
      "entryFile": "main",
      "sourceRoot": "apps/nest-jsapi/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-jsapi/tsconfig.app.json"
      }
    },
    "student-management": {
      "type": "application",
      "root": "apps/student-management",
      "entryFile": "main",
      "sourceRoot": "apps/student-management/src",
      "compilerOptions": {
        "tsConfigPath": "apps/student-management/tsconfig.app.json"
      }
    },
    "study-program": {
      "type": "application",
      "root": "apps/study-program",
      "entryFile": "main",
      "sourceRoot": "apps/study-program/src",
      "compilerOptions": {
        "tsConfigPath": "apps/study-program/tsconfig.app.json"
      }
    },
    "test-program": {
      "type": "application",
      "root": "apps/test-program",
      "entryFile": "main",
      "sourceRoot": "apps/test-program/src",
      "compilerOptions": {
        "tsConfigPath": "apps/test-program/tsconfig.app.json"
      }
    },
    "guidance-program": {
      "type": "application",
      "root": "apps/guidance-program",
      "entryFile": "main",
      "sourceRoot": "apps/guidance-program/src",
      "compilerOptions": {
        "tsConfigPath": "apps/guidance-program/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}