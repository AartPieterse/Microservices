{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/study/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "apps/study/tsconfig.app.json"
  },
  "monorepo": true,
  "study": {
    "type": "application",
    "root": "apps/study",
    "entryFile": "main",
    "sourceRoot": "apps/study/src",
    "compilerOptions": {
      "tsConfigPath": "apps/study/tsconfig.app.json"
    }
  },
  "projects": {
    "student-management": {
      "type": "application",
      "root": "apps/student-management",
      "entryFile": "main",
      "sourceRoot": "apps/student-management/src",
      "compilerOptions": {
        "tsConfigPath": "apps/student-management/tsconfig.app.json"
      }
    },
    "class-management": {
      "type": "application",
      "root": "apps/class-management",
      "entryFile": "main",
      "sourceRoot": "apps/class-management/src",
      "compilerOptions": {
        "tsConfigPath": "apps/class-management/tsconfig.app.json"
      }
    },
    "module-management": {
      "type": "application",
      "root": "apps/module-management",
      "entryFile": "main",
      "sourceRoot": "apps/module-management/src",
      "compilerOptions": {
        "tsConfigPath": "apps/module-management/tsconfig.app.json"
      }
    },
    "test-program": {
      "type": "application",
      "root": "apps/test-program",
      "entryFile": "main",
      "sourceRoot": "apps/test-program/src",
      "compilerOptions": {
        "tsConfigPath": "apps/test-program/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}